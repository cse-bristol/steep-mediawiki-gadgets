    "descriptionmsg": "categorytables-desc",
    "namemsg": "categorytables-name",
    "license-name": "MIT",
    "type": "extensions",
    "MessagesDirs": {
	"CategoryTables": [
	    "i18n"
	]
    },
    
    "AutoloadClasses": {
